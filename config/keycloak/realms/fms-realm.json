  "defaultRoles": ["viewer"],
  "requiredCredentials": ["password"],
  "passwordPolicy": "hashIterations(27500) and length(4) and notUsername",
  "otpPolicyType": "totp",
  "otpPolicyAlgorithm": "HmacSHA1",
  "otpPolicyInitialCounter": 0,
  "otpPolicyDigits": 6,
  "otpPolicyLookAheadWindow": 1,
  "otpPolicyPeriod": 30,
  "browserFlow": "browser",
